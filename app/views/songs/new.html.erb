
            <div class="main-container">
                <div class="row section">
                    <div class="col-xl-8 col-md-10 mx-auto">
                        <div class="card">
                            <div class="card-header">
                                <h6 class="card-title mb-0">Add Music</h6></div>
                            <div class="card-body">
                                <form action="#" id="addSongForm">
                                    <div class="form-row form-group">
                                        <label for="songName" class="col-md-4 text-md-right col-form-label">Song Name</label>
                                        <div class="col-md-7">
                                            <input type="text" id="songName" name="name" class="form-control">
                                        </div>
                                    </div>
                                    <div class="form-row form-group">
                                        <label for="songImage" class="col-md-4 text-md-right col-form-label">Song Cover Image</label>
                                        <div class="col-md-7">
                                            <div class="custom-file">
                                                <input type="file" class="custom-file-input" name="image" id="songImage">
                                                <label class="custom-file-label" for="songImage">Choose file</label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-row form-group">
                                        <label for="songFile" class="col-md-4 text-md-right col-form-label">Song File</label>
                                        <div class="col-md-7">
                                            <div class="custom-file">
                                                <input type="file" class="custom-file-input" name="file" id="songFile">
                                                <label class="custom-file-label" for="songImage">Choose file</label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-row form-group">
                                        <label for="artistName" class="col-md-4 text-md-right col-form-label">Artist Name</label>
                                        <div class="col-md-7">
                                            <input type="text" name="artist" id="artistName" class="form-control">
                                        </div>
                                    </div>
                                </form>
                            </div>
                            <div class="card-footer">
                                <div class="row">
                                    <div class="col-md-4"></div>
                                    <div class="col-md-7">
                                        <button type="submit" id="submit-btn" class="btn btn-brand btn-air">Save Song</button>
                                        <button type="button" class="btn btn-outline-secondary">Clear Form</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
<script type="text/javascript">
    $("#submit-btn").click(function(){
        var songName = $("#songName").val()
        var songImage = $("#songImage").val()
        var songFile = $("#songFile").val()
        var artistName = $("#artistName").val()
       $.ajax({
        url: "/songs",
        method: "POST",
        dataType: "script",
        data: {name: songName, image: songImage, file: songFile, artist: artistName},
        success: function(res){
            console.log(res)
        },
        error: function(err){
            console.log(err)
        }
    }) 
    })
    

</script>