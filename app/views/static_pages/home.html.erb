
      <%= javascript_include_tag 'static_pages' %>

      <%= stylesheet_link_tag 'static_pages' %>


      <div class="home">
        <div class="background_image" style="background-image:url(../assets/junko.jpg)"></div>
        <div class="home_container">
          <div class="container">
            <div class="row">
              <div class="col">
                <div class="home_content">
                  <div class="tags">
                    <ul class="d-flex flex-row align-items-start justify-content-start">
                      <li><a href="#">Music</a></li>
                    </ul>
                  </div>
                  <div class="home_title">
                    <h1>New song ?</h1>
                  </div>
                  <div class="home_subtitle">Check out now!</div>
                  <div class="track_info">
                    <ul class="d-flex flex-row align-items-start justify-content-start">
                      <li><a href="#">September 24, 2018</a></li>
                      <li><a href="#">Music</a></li>
                      <li>56:40</li>
                      <li><a href="#">0 Comments</a></li>
                    </ul>
                  </div>
                  <div class="track track_home">
                    <iframe width="100%" height="166" scrolling="no" frameborder="no" allow="autoplay" src="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/tracks/497269257&color=%23ff5500&auto_play=false&hide_related=false&show_comments=true&show_user=true&show_reposts=false&show_teaser=true"></iframe>

                  </div>
                  <div class="button_border home_button trans_200"><a href="#">More Info</a></div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!-- Shows -->
      <div class="shows">
        <div class="container">
          <div class="row shows_row">
            <!-- Show -->
              <%= render partial: "static_pages/song", collection: @songs %>
            <!-- Show -->

          </div>
          <div class="row">
            <div class="col text-center">
              <div class="button_fill shows_button"><a href="#">browse musics</a></div>
            </div>
          </div>
        </div>
      </div>
      <!-- Bi Weekly -->
      <!-- Shows 2 -->
      <div class="shows_2">
        <div class="container">
          <div class="row">
            <div class="col">
              <div class="shows_2_title">By Category</div>
            </div>
          </div>
          <div class="row shows_2_row">
            <!-- Show -->
            <%= render partial: "static_pages/category", collection: @categories %>
            <!-- Show -->
          </div>
          <div class="row">
            <div class="col text-center">
              <div class="button_fill shows_2_button"><a href="#">browse genres</a></div>
            </div>
          </div>
        </div>
      </div>
